          "text" : "<!DOCTYPE html>\n<html lang=\"en\">\n  <head>\n    <meta charset=\"utf-8\">\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1\">\n    <meta name=\"description\" content=\"\">\n    <meta name=\"author\" content=\"\">\n    <link rel=\"icon\" href=\"favicon.ico\">\n\n    <title>P-Eproc : </title>\n    <base href=\"https://p-eproc.pelindo.co.id/\">\n    \n    <!-- DATATABLE -->\n    <script type=\"text/javascript\" language=\"javascript\" src=\"libraries/DataTables-1.10.7/media/js/jquery.js\"></script>\n\n    <!-- Bootstrap core CSS -->\n    <link href=\"libraries/bootstrap/dist/css/bootstrap.css\" rel=\"stylesheet\">    \n    \n    <!-- Custom styles for this template -->\n    <link href=\"libraries/bootstrap/css/navbar-fixed-top.css\" rel=\"stylesheet\">\n    \n    <!-- CHAT BOX -->\n    <link rel=\"stylesheet\" href=\"css/gaya.css\" type=\"text/css\">\n    <link rel=\"stylesheet\" href=\"css/gaya-bootstrap.css\" type=\"text/css\">\n    <link rel=\"stylesheet\" href=\"css/gaya-table-responsive.css\" type=\"text/css\">\n    <style type=\"text/css\">\n        .items {\n            width: 90%;\n            margin: 0px auto;\n        }\n\n        .slick-slide img {\n            width: 100%;\n            border: 0px solid #fff\n        }\n    </style>\n     <!--Slick Carousel Slider-->\n    <link rel=\"stylesheet\" type=\"text/css\" href=\"https://cdn.jsdelivr.net/gh/kenwheeler/slick@1.8.1/slick/slick-theme.css\" />\n    <link rel=\"stylesheet\" type=\"text/css\" href=\"https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css\" />\n    <script type=\"text/javascript\" src=\"https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js\"></script>\n    <link href=\"https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/css/select2.min.css\" rel=\"stylesheet\" />\n    <script src=\"https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/js/select2.min.js\"></script>\n\n    <!-- STYLE MASING2 PERUSAHAAN -->\n        \n    <!-- FONT AWESOME -->\n    <link rel=\"stylesheet\" href=\"libraries/font-awesome-4.7.0/css/font-awesome.css\" type=\"text/css\">\n    \n    <!-- PAGINATION -->\n    <link rel=\"stylesheet\" type=\"text/css\" href=\"libraries/drupal/index.css\" />     \n    \n    <script language=\"javascript\" src=\"js/jquery.redirect.js\"></script>\n    \n    <script src=\"libraries/multifile-master/jquery.MultiFile.js\"></script>\n            \n    <!-- VERTICAL CSS3 MENU -->\n    <link href=\"libraries/vertical-css3-menu-source/css/style.css\" media=\"screen\" rel=\"stylesheet\">    \n\n    <!-- Firebase -->\n    <script src=\"https://www.gstatic.com/firebasejs/6.3.4/firebase-app.js\"></script>  \n    <script src=\"https://www.gstatic.com/firebasejs/6.3.4/firebase-messaging.js\"></script>\n\n    <script>\n      // Initialize Firebase\n      var firebaseConfig = {\n        apiKey: \"AIzaSyAmH_xMkqt126IghY264C5FKpm_URK8ilI\",\n        authDomain: \"p3mprocurement.firebaseapp.com\",\n        databaseURL: \"https://p3mprocurement.firebaseio.com\",\n        projectId: \"p3mprocurement\",\n        storageBucket: \"p3mprocurement.appspot.com\",\n        messagingSenderId: \"69596577464\",\n        appId: \"1:69596577464:web:7ccc3b76581aba29\"\n      };\n\n      firebase.initializeApp(firebaseConfig);\n\n      // Retrieve Firebase Messaging object.\n      const messaging = firebase.messaging();\n\n\n\n      navigator.serviceWorker.register('firebase-messaging-sw.js')\n      .then((registration) => {\n        messaging.useServiceWorker(registration);\n        requestPermission()\n        \n      });\n\n      function requestPermission() {\n        messaging.requestPermission().then(function() {      \n          console.log('Notification permission granted.');\n          \n          registerToken();\n        }).catch(function(err) {\n          console.log('Unable to get permission to notify.', err);\n        });\n      }\n\n\n     // Registered\n     function registerToken(){\n        //  console.log('getToken');\n       return messaging.getToken()\n          .then(function(currentToken) {\n            if (currentToken) {\n            //   console.log(currentToken);\n              $(\"#reqToken\").val(currentToken);\n            } else{\n                console.log(currentToken);\n            }\n          }).catch(function(err) {\n            console.log('An error occurred while retrieving token. ', err);\n          });\n      }\n\n      // ForeGround\n      messaging.onMessage(function(payLoad)\n      {\n        console.log(\"Message Received\",payLoad.data);\n       // getNotif();\n\n        if(payLoad.data.type == 'CHATBOX' && payLoad.data.jenis == 'CHAT'){\n            refreshChatbox();\n        }\n\n        var notifData=payLoad.notification;\n        // try{    \n        //     notifData=JSON.parse(payLoad.data.notification);            \n        // }catch(err)\n        // {\n        //     console.log(err);\n        //     notifData={\n        //         title:\"Pesan Baru P-EProcurement\",\n        //         body:\"\"\n        //     };\n        // }\n\n        notificationTitle = notifData.title;\n        notificationOptions = {\n          body: notifData.body,\n          icon: 'images/logo-header-new.png',\n        //   image: 'images/logo-header-new.png',\n        };\n        var notification = new Notification(notificationTitle, notificationOptions);\n      });\n\n     \n      function subscribeTokenToTopic(token, topic) {\n        fetch('https://iid.googleapis.com/iid/v1/'+token+'/rel/topics/'+topic, {\n            method: 'POST',\n            headers: new Headers({\n              'Authorization': 'key=AAAAEDRHfrg:APA91bFAVakF2JvGNaLizZ3_XW90EPF-Wfz2vEpGV7cKmF4hYpasRlIWMl0JVzYy0KUqAtrNYF8TWG0b41kmPaUGbvS2YnX71B0gcW8zncKmLo2GQ6wsquBk7MJMu2oXYhyrRJuZp05X'\n            })\n        }).then(response => {\n            if (response.status < 200 || response.status >= 400) {\n                throw 'Error subscribing to topic: '+response.status + ' - ' + response.text();\n            }\n            console.log('Subscribed to \"'+topic+'\"');\n            console.log(response.text());\n        }).catch(error => {\n            console.error(error);\n        })\n      }\n    </script>\n    <!-- Firebase -->    \n  </head>\n<body class=\"home\" >\n      \n\n    <div class=\"area-shortcut\">\n        <ul class=\"icon-menu\">\n            <!--<li class=\"icon-box home\">\n                <a onClick=\"openCabang('app/loadUrl/main/cabang');\">\n                    <img src=\"images/icon-cabang.png\">\n                    <span>Cabang</span>\n                    <h2>Tampilkan Cabang &amp; Anak Perusahaan</h2>\n                </a>\n            </li>-->\n                        \n            \n          \n            \n                        \n        </ul>\n    </div>\n    \n    \n    <!-- Fixed navbar -->\n    <nav class=\"navbar navbar-default navbar-fixed-top\">\n        <div class=\"container container-aps-header\">\n            <div class=\"area-header-utama\">\n                <div class=\"area-header-utama-inner\">\n                    <div class=\"navbar-header\">\n                        <button type=\"button\" class=\"navbar-toggle collapsed\" data-toggle=\"collapse\" data-target=\"#navbar\" aria-expanded=\"false\" aria-controls=\"navbar\">\n                            <span class=\"sr-only\">Toggle navigation</span>\n                            <span class=\"icon-bar\"></span>\n                            <span class=\"icon-bar\"></span>\n                            <span class=\"icon-bar\"></span>\n                        </button>\n                        \n                        <!-- LOGO MASING2 PERUSAHAAN -->\n                                                \n                        <a class=\"navbar-brand area-logo\" style=\"height : 70px\" href=\"app/index/?reqPerusahaan=group\" title=\"PelindO\">\n                            \n   